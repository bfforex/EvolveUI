# EvolveUI Environment Configuration Example
# Copy this file to .env and configure as needed

# Backend Configuration
LOG_LEVEL=INFO
OLLAMA_HOST=http://localhost:11434
CHROMADB_HOST=localhost
CHROMADB_PORT=8001

# Search Engine Configuration
SEARCH_DEFAULT_ENGINE=duckduckgo

# SearXNG Configuration (optional)
SEARXNG_INSTANCE_URL=https://searx.be

# Google Custom Search API (optional - requires API key)
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_CX=your_custom_search_engine_id_here

# Bing Search API (optional - requires API key)
BING_API_KEY=your_bing_api_key_here

# Frontend Configuration
REACT_APP_BACKEND_URL=http://localhost:8000
REACT_APP_DEBUG=false

# Docker Configuration
COMPOSE_PROJECT_NAME=evolveui